{
  "restartable": "rs",
  "ignore": [
      ".git",
      ".svn",
      "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
      "js": "node --harmony"
  },
  "watch": [
    "*.handlebars",
    "*.js",
    "*.json"
  ],
  "env": {
      "NODE_ENV": "development"
  },
  "ext": "js json handlebars"
}
